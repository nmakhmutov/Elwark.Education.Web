<section class="container">
    <header Class="header">
        @Header
    </header>

    <article class="content">
        @Content
    </article>

    <aside class="sidebar">
        @Sidebar
    </aside>
</section>

@code {

    [Parameter]
    public RenderFragment? Header { get; set; }

    [Parameter]
    public RenderFragment? Content { get; set; }

    [Parameter]
    public RenderFragment? Sidebar { get; set; }

}
